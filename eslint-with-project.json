{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "project": "coc.nvim/tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/no-unnecessary-type-assertion": 2
  }
}
